{
    "name": "laravelir/smsir",
    "description": "ir sms services package",
    "homepage": "https://github.com/laravelir/smsir",
    "type": "library",
    "version": "0.1.0",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel support",
        "lumen packages",
        "laravel packages",
        "lumen support",
        "laravel sms",
        "laravelir sms"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com",
            "role": "maintainer",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravelir\\SMSIR\\": "src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelir\\SMSIR\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelir\\SMSIR\\Providers\\SMSIRServiceProvider"
            ],
            "aliases": {
                "SMSIR": "Laravelir\\SMSIR\\Facades\\SMSIR"
            }
        }
    },
    "require": {
        "php": ">=7.4|^8.0"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
